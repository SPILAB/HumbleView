<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HumbleImageView">
        <attr name="url" format="string" />
        <attr name="offlineCache" format="boolean" />
        <attr name="debugFlags" format="string" />
        <attr name="loadedImageScaleType" format="enum">
            <enum name="matrix" value="0" />
            <enum name="fitXY" value="1" />
            <enum name="fitStart" value="2" />
            <enum name="fitCenter" value="3" />
            <enum name="fitEnd" value="4" />
            <enum name="center" value="5" />
            <enum name="centerCrop" value="6" />
            <enum name="centerInside" value="7" />
        </attr>
        <attr name="transform" format="string" />
        <attr name="transformValues" format="string" />
    </declare-styleable>
</resources>